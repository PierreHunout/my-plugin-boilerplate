{
	"extends": ["plugin:@wordpress/eslint-plugin/recommended", "prettier"],
	"plugins": ["prettier"],
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jquery": true
	},
	"globals": {
		"wp": "readonly",
		"ajaxurl": "readonly"
	},
	"rules": {
		"prettier/prettier": "error",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"camelcase": "off",
		"@wordpress/no-global-active-element": "off"
	}
}
